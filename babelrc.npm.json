{
  "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".ts", ".tsx", ".js", ".jsx"],
        "root": ["./src"]
      }
    ]
  ],
  "ignore": [
    "**/__snapshots__",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/react-app-env.d.ts",
    "**/setupTests.ts",
    "**/demo",
    "./src/index.tsx"
  ]
}
